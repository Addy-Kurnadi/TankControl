<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TankControl.View.ComponentGDA" x:Class="TankControl.View.ComponentGDA.TinyTankPumpComponent" 
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="275">
    <Grid RenderTransformOrigin="0.5,0.5" Margin="0,0.001,0,0">
    	<Grid.RenderTransform>
    		<TransformGroup>
    			<ScaleTransform/>
    			<SkewTransform/>
    			<RotateTransform Angle="0.08"/>
    			<TranslateTransform/>
    		</TransformGroup>
    	</Grid.RenderTransform>
    	<local:TinyTankComponent x:Name="TinyTank" HorizontalAlignment="Left" Margin="4,-7.249,0,0" VerticalAlignment="Top" />
    	<local:BigValveLComponent x:Name="BigValve" HorizontalAlignment="Left" Margin="153.332,16.332,0,0" VerticalAlignment="Top"/>
    	<local:SmallValveComponent x:Name="SmallValve" HorizontalAlignment="Left" Margin="145.335,46.109,0,-1.506" VerticalAlignment="Top"/>
    	<local:PumpComponent x:Name="Pump" HorizontalAlignment="Right" Margin="0,6.111,114.583,0" VerticalAlignment="Top"/>
            
    </Grid>
</UserControl>
